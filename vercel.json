{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "npm install",
  "installCommand": "npm install -g pg",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [{
    "path": "/cron",
    "schedule": "5 * * * *"
  }]
}